{
    "actorSpecification": 1,
    "name": "youtube-audio-extractor",
    "title": "YouTube Audio Extractor with R2 Upload",
    "description": "Extract audio-only streams from YouTube videos and upload directly to Cloudflare R2 with IP rotation and anti-bot detection. Uses yt-dlp for reliable extraction.",
    "version": "1.2.0",
    "buildTag": "latest",
    "dockerfile": "./Dockerfile",
    "environmentVariables": {},
    "readme": "./README.md",
    "input": "./input_schema.json",
    "defaultRunOptions": {
        "build": "latest",
        "timeoutSecs": 300,
        "memoryMbytes": 512
    },
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Extracted Results",
            "description": "Results from YouTube extraction or proxy requests",
            "views": {
                "overview": {
                    "title": "Overview",
                    "transformation": {
                        "fields": [
                            "videoId",
                            "url",
                            "method",
                            "extractedAt",
                            "success",
                            "proxyInfo"
                        ]
                    }
                }
            }
        }
    }
}