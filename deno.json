{
  "name": "youtube-video-parser",
  "description": "Complete YouTube video parser and proxy - bypasses yt-dlp entirely",
  "version": "1.0.0",
  "tasks": {
    "dev": "deno run --watch --allow-net --allow-env main.ts",
    "start": "deno run --allow-net --allow-env main.ts",
    "test": "deno test --allow-net",
    "format": "deno fmt",
    "lint": "deno lint"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "deno.ns",
      "deno.unstable"
    ]
  },
  "deploy": {
    "project": "proxy-function",
    "entrypoint": "main.ts",
    "include": [
      "main.ts",
      "youtube-parser.ts",
      "deno.json"
    ],
    "exclude": [
      "tests/**",
      "enhanced-nodejs-integration.js",
      "example-nodejs-integration.js",
      "README-new.md",
      "*.md"
    ]
  }
}